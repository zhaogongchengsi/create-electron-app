{
  "compilerOptions": {
    "target": "esnext",
    "module": "ESNext",
    "rootDir": ".",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "useUnknownInCatchVariables": true,
    "noImplicitOverride": true,
    "skipLibCheck": true,
    "noEmit": true,
  },
  "exclude": ["./templates"]
}
