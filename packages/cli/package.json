{
  "name": "@zzhaon/create-electron-app",
  "type": "module",
  "version": "0.3.4",
  "description": "Create a cli for electron app",
  "author": "zhaozunhong",
  "license": "ISC",
  "homepage": "https://github.com/zhaogongchengsi/create-electron-app/tree/master/packages/cli#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhaogongchengsi/create-electron-app.git",
    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/zhaogongchengsi/create-electron-app/issues"
  },
  "keywords": [
    "electron",
    "create-electron-app",
    "esbuild",
    "vite"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./picture-loader": {
      "types": "./dist/file-loader.d.ts",
      "import": "./dist/file-loader.mjs",
      "require": "./dist/file-loader.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "create-electron-app": "./dist/cli.mjs",
    "cea": "./dist/cli.mjs"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": "^16.0.0 || >=18.0.0"
  },
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "prepublish": "pnpm run build",
    "release": "npm publish",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@rspack/core": "^0.3.1",
    "c12": "^1.4.2",
    "cac": "^6.7.14",
    "chokidar": "^3.5.3",
    "citty": "^0.1.3",
    "consola": "^3.2.3",
    "electron-builder": "^24.6.3",
    "file-loader": "^6.2.0",
    "node-fetch": "^3.2.10",
    "pathe": "^1.1.1",
    "perfect-debounce": "^1.0.0",
    "pkg-types": "^1.0.3",
    "vite": "^4.4.9",
    "webpackbar": "^5.0.2"
  },
  "devDependencies": {
    "@types/node": "^18.7.13",
    "unbuild": "^0.8.11"
  }
