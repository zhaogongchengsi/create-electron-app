{
  "compilerOptions": {
    "target": "esnext",
    "module": "ESNext",
    "rootDir": ".",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    // "declaration": false,
    // "declarationDir": "./dist/types",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "useUnknownInCatchVariables": true,
    "noImplicitOverride": true,
    "skipLibCheck": true,
    "noEmit": true
  },
  "exclude": ["dist/**/*", "**/test", ]
}
